{
    "name": "veeroll-package/composer",
    "type": "project",
    "description": "veeroll package for creating video.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.272",
        "barryvdh/laravel-dompdf": "^2.0",
        "convertkit/convertkitapi": "^1.2",
        "doctrine/dbal": "^3.6",
        "gumlet/php-image-resize": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "hashids/hashids": "^5.0",
        "laravel/cashier": "^14.12",
        "laravel/framework": "^10.30",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravelcollective/html": "^6.4",
        "league/flysystem-aws-s3-v3": "^3.15",
        "maatwebsite/excel": "^3.1",
        "openai-php/client": "^0.7.8",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.4",
        "spatie/laravel-permission": "^5.10"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan passport:keys --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Vendor\\PackageName\\PackageServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
